{
  "name": "local-dynamics/laravel-revisionable",
  "license": "MIT",
  "description": "Keep a revision history for your models without thinking, created as a package for use with Laravel",
  "keywords": [
    "model",
    "laravel",
    "revision",
    "audit",
    "history"
  ],
  "homepage": "http://github.com/local-dynamics/laravel-revisionable",
  "authors": [
    {
      "name": "Thomas EnÃŸner",
      "email": "thomas@ld-n.de"
    },
    {
      "name": "Chris Duell",
      "email": "me@chrisduell.com"
    }
  ],
  "require": {
    "php": ">=8.4",
    "illuminate/support": "^v11.0|^v12.0",
    "laravel/framework": "^v11.0|^12.1.1",
    "ext-json": "*"
  },
  "autoload": {
    "classmap": [
      "src/migrations"
    ],
    "psr-4": {
      "LocalDynamics\\Revisionable\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/migrations"
    ],
    "psr-4": {
      "LocalDynamics\\Revisionable\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^10.1.0",
    "laravel/pint": "^v1.21.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "LocalDynamics\\Revisionable\\ServiceProvider"
      ]
    }
  }
}
