{
    "name": "local-dynamics/laravel-revisionable",
    "license": "MIT",
    "description": "Keep a revision history for your models without thinking, created as a package for use with Laravel",
    "keywords": [
        "model",
        "laravel",
        "revision",
        "audit",
        "history"
    ],
    "homepage": "http://github.com/local-dynamics/revisionable",
    "authors": [
        {
            "name": "Thomas EnÃŸner",
            "email": "thomas@ld-n.de"
        },
        {
            "name": "Chris Duell",
            "email": "me@chrisduell.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^8.0|^9.0",
        "laravel/framework": "^8.0|^9.0",
        "ext-json": "*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "LocalDynamics\\Revisionable\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/migrations"
        ],
        "psr-4": {
            "LocalDynamics\\Revisionable\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "~4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LocalDynamics\\Revisionable\\ServiceProvider"
            ]
        }
    }
}
